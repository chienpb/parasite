   <children>
      <Button layoutX="105.0" layoutY="21.0" mnemonicParsing="false" onAction="#captureImage" text="Capture" />
      <ImageView fx:id="imageView" fitHeight="216.0" fitWidth="384.0" layoutX="58.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="345.0" layoutY="21.0" mnemonicParsing="false" onAction="#copyToClipboard" text="Copy" />
   </children>
</AnchorPane>
